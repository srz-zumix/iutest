{
  "name": "iutest",
  "version": "1.0.0",
  "description": "",
  "dependencies": {},
  "devDependencies": {
    "eclint": "^2.8.1",
    "editorconfig-checker": "^1.3.3",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "lint:editorconfig": "editorconfig-checker --exclude-regexp \"\\.sln|\\..*proj\" .",
    "lint:editorconfig-cpp": "editorconfig-checker src include",
    "eclint": "npm-run-all eclint:*",
    "eclint:src": "eclint check ./src",
    "eclint:include": "find ./include -type f -not -name iutest_ver.hpp | xargs eclint check -s 2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srz-zumix/iutest.git"
  },
